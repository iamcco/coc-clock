{
  "name": "coc-status",
  "version": "0.0.1",
  "description": "notification for coc",
  "keywords": [
    "coc.nvim",
    "notification",
    "status"
  ],
  "main": "./out/index.js",
  "repository": "https://github.com/iamcco/coc-status",
  "author": "iamcco <ooiss@qq.com>",
  "license": "MIT",
  "private": false,
  "engines": {
    "coc": "^0.0.71"
  },
  "activationEvents": [
    "*"
  ],
  "contributes": {
    "configuration": {
      "title": "Coc-status Configuration",
      "type": "object",
      "properties": {
        "coc-status.enable": {
          "type": "boolean",
          "default": true,
          "description": "Enable notification"
        },
        "coc-status.trace.server": {
          "type": "string",
          "default": "off",
          "enum": [
            "off",
            "messages",
            "verbose"
          ],
          "description": "Trace level of coc-status"
        }
      }
    }
  },
  "scripts": {
    "build": "tsc -p ./"
  },
  "devDependencies": {
    "@types/node": "^12.0.4",
    "coc.nvim": "^0.0.71",
    "typescript": "^3.4.5"
  },
  "dependencies": {
    "vscode-languageserver-protocol": "^3.14.1"
  }
}
