{
  "name": "coc-clock",
  "version": "0.0.3",
  "description": "Big Clock for Neovim",
  "keywords": [
    "coc.nvim",
    "clock",
    "neovim"
  ],
  "main": "./out/index.js",
  "repository": "https://github.com/iamcco/coc-clock",
  "author": "iamcco <ooiss@qq.com>",
  "license": "MIT",
  "private": false,
  "engines": {
    "coc": "^0.0.71"
  },
  "activationEvents": [
    "*"
  ],
  "contributes": {
    "configuration": {
      "title": "Coc-clock Configuration",
      "type": "object",
      "properties": {
        "clock.enable": {
          "type": "boolean",
          "default": false,
          "description": "Enable clock"
        },
        "clock.color": {
          "type": "string",
          "default": "",
          "description": "clock color, default is Normal highlight group"
        },
        "clock.winblend": {
          "type": "number",
          "default": 100,
          "description": "config opacity of floating window background 0-100 from fully opaque to transparent, default is 100"
        },
        "clock.top": {
          "type": "number",
          "default": 1,
          "description": "position distance to top"
        },
        "clock.right": {
          "type": "number",
          "default": 1,
          "description": "position distance to right"
        },
        "clock.trace.server": {
          "type": "string",
          "default": "off",
          "enum": [
            "off",
            "messages",
            "verbose"
          ],
          "description": "Trace level of coc-clock"
        }
      }
    }
  },
  "scripts": {
    "build": "tsc -p ./"
  },
  "devDependencies": {
    "@types/node": "^12.0.4",
    "coc.nvim": "^0.0.71",
    "typescript": "^3.4.5"
  },
  "dependencies": {
    "rxjs": "^6.5.2",
    "vscode-languageserver-protocol": "^3.14.1"
  }
}
